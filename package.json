{
  "name": "hdb-resale",
  "version": "1.0.0",
  "description": "React version of web visualization project",
  "main": "index.js",
  "scripts": {
    "start-prod": "npm run build & npm run start-server",
    "start-dev": "webpack-dev-server --host 0.0.0.0 --port 9000 --progress --colors --hot --config ./webpack.config.js",
    "start-server": "node server_dist/server.js",
    "build": "npm run clean & npm run build-server & npm run build-js",
    "build-server": "babel server_src --out-dir server_dist",
    "clean": "rm -rf server_dist",
    "build-js": "webpack --progress --colors --config ./webpack.config.js"
  },
  "contributors": [
    {
      "name": "Yong Jun"
    },
    {
      "name": "Yap Hong Kheng"
    }
  ],
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "jStat": "^1.5.3",
    "lodash.sortbyorder": "^3.4.4",
    "loess": "^1.3.1",
    "mongoose": "^4.5.7",
    "node-fetch": "^1.5.3",
    "proj4": "^2.3.14",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-router": "^2.6.1"
  }
}
